@model IEnumerable<PROJECT_MVC.MVC_Tab>
@{
    ViewBag.Title = "Display_Load";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<center>
    <h2>Display All Details</h2>
    <div>
        @using (Html.BeginForm("Display_Load", "DisplayAll", FormMethod.Get))
        {
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(m => m.Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(m => m.Age)
                    </th>
                    <th>
                        @Html.DisplayNameFor(m => m.Address)
                    </th>
                    <th>
                        @Html.DisplayNameFor(m => m.Email)
                    </th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(mitem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(mitem => item.Age)
                        </td>
                        <td>
                            @Html.DisplayFor(mitem => item.Address)
                        </td>
                        <td>
                            @Html.DisplayFor(mitem => item.Email)
                        </td>
                    </tr>
                }
            </table>
        }
    </div>
</center>


